{
    "variables": {
        "access_key": "",
        "secret_key": "",
        "vpcid_to_build_in": "",
        "subnetid_to_build_in": "",
        "tag_application": "",
        "tag_contact_email": "",
        "tag_customer": "",
        "tag_team": "",
        "tag_environment": ""
    },
    "builders": [{
        "type": "googlecompute",
        "project_id": "cloud-arch",
        "source_image": "ubuntu-1604-xenial-v20180814",
        "ssh_username": "ubuntu",
        "zone": "us-east1-b",
        "image_name": "rabbitmq {{timestamp}}",
        "tags": {
            "Name": "rabbitmq packer image builder",
            "application": "{{user `tag_application`}}",
            "contact-email": "{{user `tag_contact_email`}}",
            "customer": "{{user `tag_customer`}}",
            "team": "{{user `tag_team`}}",
            "environment": "{{user `tag_environment`}}"
        }
    }],
    "provisioners": [{
            "type": "file",
            "source": "erlang",
            "destination": "/home/ubuntu/erlang"
        },
        {
            "type": "file",
            "source": "limits.conf",
            "destination": "/home/ubuntu/limits.conf"
        },
        {
            "type": "shell",
            "execute_command": "{{.Vars}} bash '{{.Path}}'",
            "script": "provision.sh"
        }
    ]
}